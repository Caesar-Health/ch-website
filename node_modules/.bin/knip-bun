#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules/knip/bin/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules/knip/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules/knip/bin/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules/knip/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/knip@5.66.4_@types+node@24.9.2_typescript@5.9.3/node_modules:/Users/shoukrikattan/Development/ch-caesar-health/ch-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../knip/bin/knip-bun.js" "$@"
else
  exec bun  "$basedir/../knip/bin/knip-bun.js" "$@"
fi
